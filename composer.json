{
  "name": "free-elephants/rest-daemon",
  "description": "PHP http socket REST server",
  "bin": ["bin/rest-daemon"],
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "require": {
    "amphp/aerys": "^0.7.1",
    "cboden/ratchet": "0.4",
    "free-elephants/di": "^2.0",
    "guzzlehttp/psr7": "^1.0",
    "monolog/monolog": "^1.21",
    "oscarotero/psr7-middlewares": "^3.18",
    "psr/container": "^1.0",
    "relay/middleware": "^1.1",
    "relay/relay": "~1.0",
    "symfony/routing": "^3.1",
    "zendframework/zend-code": "^3.1",
    "zendframework/zend-diactoros": "^1.3",
    "zendframework/zend-http": "^2.5",
    "zircote/swagger-php": "^2.0",
    "psr/log": "^1.0",
    "symfony/console": "^4.2"
  },
  "require-dev": {
    "codeception/codeception": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "FreeElephants\\RestDaemon\\": "src/FreeElephants/RestDaemon/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FreeElephants\\": "tests/unit/FreeElephants/",
      "RestDaemon\\Example\\": "example/src/"
    }
  },
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "samizdam",
      "email": "samizdam@inbox.ru"
    }
  ],
  "suggest": {
    "free-elephants/di": "Simplest PHP Dependency Injector. "
  }
}
